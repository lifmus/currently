<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script>
      var _rollbarParams = {"server.environment": "<%= Rails.env %>"};
      _rollbarParams["notifier.snippet_version"] = "2"; var _rollbar=["e607f76c16344dc6aee79da1f19202c5", _rollbarParams]; var _ratchet=_rollbar;
