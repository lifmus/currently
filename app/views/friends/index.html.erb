<div class='container'>
  <div class='row-fluid'>
    <div class='span9'>
      <h1>Friends</h1>
    </div>
    <div class='span3'>  
      <div class='well sidebar-nav'>  
        <ul class='nav nav-list'>  
          <li class='nav-header'>Facebook Friends</li>  
          <li class='divider'></li>
          <% @facebook_friends.each do |f| %>
            <li>
              <%= link_to f.name, user_path(f.slug) %>
              <a href='#' class='btn'>Follow</a>
            </li>
          <% end %>
        </ul>  
      </div> 
    </div>
  </div>
</div>